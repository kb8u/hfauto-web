<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>Palstar HF-Auto</title>
  <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
  <link rel="stylesheet" href="hfauto.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
  <script src="bootstrap-toolkit.min.js"></script>
  <script src="https://www.gstatic.com/charts/loader.js"></script>
  <script>
    // The URI to use for the websocket json feed.
    var wsUri = '<%= $wsUri %>';
  </script>
  <script src="hfauto.js"></script>
</head>
<body>
  <div class="container-fluid">
    <div class="row row-eq-height">
      <div class="col-sm-6">
        <div class="well well-sm clearfix">
          <div class="col-sm-4" id="SWR"></div>
          <div class="col-sm-4" id="L"></div>
          <div class="col-sm-4" id="C"></div>
        </div>
      </div>
      <div class="col-sm-6">
        <div class="well well-sm clearfix">
          <div class="col-sm-4" id="Power"></div>
          <div class="col-sm-4" id="PeakPower"></div>
          <div class="col-sm-4" id="PowerRange">
            <div class="radioGroup">
              <div class="radio">
                <label><input type="radio" name="powerRange" class="powerRadio">1800 W</label>
              </div>
              <div class="radio">
                <label><input type="radio" name="powerRange" class="powerRadio">180 W</label>
              </div>
              <div class="radio">
                <label><input type="radio" name="powerRange" class="powerRadio">18 W</label>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row row-eq-height">
      <div class="col-sm-6">
        <div class="well well-sm clearfix">
          <div class="col-sm-4">
            <p class="w1trText" id="Antenna"></p>
            <p class="descrText">Antenna</p>
          </div>
          <div class="col-sm-4">
            <p class="w1trText" id="OutputJack"></p>
            <p class="descrText">Output Jack</p>
          </div>
          <div class="col-sm-4">
            <p class="w1trText" id="Frequency"></p>
            <p class="descrText">kHz</p>
          </div>
        </div>
      </div>
      <div class="col-sm-6">
        <div class="well well-sm clearfix">
          <div class="col-sm-4">
            <p class="w1trText" id="OutputJackMode"></p>
            <p class="descrText">Output Jack Mode</p>
          </div>
          <div class="col-sm-4">
            <p class="w1trText" id="TunerMode"></p>
            <p class="descrText">Tuner Mode</p>
          </div>
          <div id="OnAir" class="col-sm-4 alert alert-danger">
            ON AIR
          </div>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
